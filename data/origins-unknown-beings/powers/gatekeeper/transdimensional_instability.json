            {
                "type": "origins:execute_command",
                "command": "scoreboard players set @s o-u-b.deaths 0"
            }
        ]
    },
    "condition": {
        "type": "origins:command",
        "command": "execute if score @s o-u-b.deaths matches 1..",
        "comparison": "==",
        "compare_to": 1
    }
}